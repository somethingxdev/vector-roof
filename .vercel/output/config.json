{
	"version": 3,
	"routes": [
		{
			"src": "^/_astro/(.*)$",
			"headers": {
				"cache-control": "public, max-age=31536000, immutable"
			},
			"continue": true
		},
		{
			"handle": "filesystem"
		},
		{
			"src": "^\\/_image$",
			"dest": "_render"
		},
		{
			"src": "^\\/api\\/sendEmail\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/api\\/sendEmailFooter\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/news\\/([^/]+?)\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/projects\\/category\\/([^/]+?)\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/projects\\/([^/]+?)\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/$",
			"dest": "_render"
		},
		{
			"src": "^\\/company\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/news\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/projects\\/?$",
			"dest": "_render"
		}
	],
	"images": {
		"sizes": [
			640,
			750,
			828,
			1080,
			1200,
			1920,
			2048,
			3840
		],
		"domains": [],
		"remotePatterns": [
			{
				"protocol": "https"
			}
		]
	}
}